{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gigi-mobile-i2",
  "version": "0.1.0",
  "identifier": "app.gigi.i2",
  "build": {
    "beforeDevCommand": "bun run dev:i2",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "gigi-mobile - I2",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}